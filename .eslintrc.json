{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
        "indent": ["error", 4, {
            "SwitchCase": 1
        }],
        "react/jsx-indent": [2, 4],
        "react/jsx-indent-props": [2, 4]
    },
    "env": {
        "browser": true
    },
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ]
}
